<!doctype html>
<html lang="en">
<head>
  <%- include ("../partials/header.ejs") %>
</head>
<body>
  <h1>Babbage Difference Engine No. 2</h1>
  <form id="set-engine">
    <label for="columns">Columns: </label>
	<input type="number" name="columns" id="columns" value="3" min="2" max="8" required="required" />
	<label for="units">Units: </label>
	<input type="number" name="units" id="units" value="5" min="2" max="31" required="required" />
	<label for="style-up">Style Up? </label>
	<input type="checkbox" name="style-up" id="style-up" value="1" checked="checked" />
	<input type="submit" id="set-engine-submit" name="set-engine-submit" value="Build Engine" />
  </form>
  <div id="build-engine">
  </div>
  <div id="control-engine">
  </div>
  <div id="monitor-engine">
  </div>
  <div id="share-engine">
  </div>
</body>
</html>
